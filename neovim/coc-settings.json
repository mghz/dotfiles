{
    "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "explorer.position": "floating",
  "explorer.quitOnOpen": true,
  "explorer.icon.enableVimDevicons": true
}
